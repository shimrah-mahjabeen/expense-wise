{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base",
        "plugin:node/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": [
            "warn",
            "double"
        ],
        "no-unused-vars": "warn",
        "func-names": "off",
        "no-process-exit": "off",
        "implicit-arrow-linebreak": "off",
        "operator-linebreak": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off"
    }
}
